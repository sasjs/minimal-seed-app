{
  "$schema": "https://raw.githubusercontent.com/sasjs/utils/main/src/types/sasjsconfig-schema.json",
  "macroFolders": ["sasjs/macros"],
  "programFolders": [],
  "serviceConfig": {
    "serviceFolders": ["sasjs/services/common"]
  },
  "defaultTarget": "viya",
  "targets": [
    {
      "name": "viya",
      "serverUrl": "https://sas.analytium.co.uk",
      "serverType": "SASVIYA",
      "appLoc": "/Public/app/minimal-seed-app",
      "buildConfig": {
        "buildOutputFileName": "myviyadeploy.sas"
      },
      "streamConfig": {
        "streamWeb": true,
        "streamWebFolder": "webv",
        "webSourcePath": "src"
      },
      "deployConfig": {
        "deployServicePack": true
      }
    },
    {
      "name": "sas9",
      "serverType": "SAS9",
      "appLoc": "/Public/app/minimal-seed-app",
      "buildConfig": {
        "buildOutputFileName": "mysas9deploy.sas"
      },
      "deployConfig": {
        "deployServicePack": true
      },
      "serverName": "SASApp",
      "repositoryName": "Foundation",
      "streamConfig": {
        "streamWeb": true,
        "streamWebFolder": "web9",
        "webSourcePath": "src"
      }
    }
  ]
}

{
  "$schema": "https://raw.githubusercontent.com/sasjs/utils/main/src/types/sasjsconfig-schema.json",
  "macroFolders": [
    "sasjs/macros"
  ],
  "programFolders": [],
  "serviceConfig": {
    "serviceFolders": [
      "sasjs/services/common"
    ]
  },
  "defaultTarget": "viya",
  "targets": [
    {
      "name": "viya",
      "serverUrl": "https://sas.analytium.co.uk",
      "serverType": "SASVIYA",
      "appLoc": "/Public/app/minimal-seed-app",
      "buildConfig": {
        "buildOutputFileName": "myviyadeploy.sas"
      },
      "streamConfig": {
        "assetPaths": [],
        "streamWeb": true,
        "streamWebFolder": "webv",
        "webSourcePath": "src"
      },
      "deployConfig": {
        "deployServicePack": true,
        "deployScripts": [
        ]
      }
    },
    {
      "name": "sas9",
      "serverType": "SAS9",
      "serverUrl": "YOURSAS9SERVER",
      "appLoc": "/Public/app/minimal-seed-app",
      "buildConfig": {
        "buildOutputFileName": "mysas9deploy.sas"
      },
      "deployConfig": {
        "deployScripts": [
          "sasjsbuild/myviyadeploy.sas"
        ]
      },
      "serverName": "SASApp",
      "repositoryName": "Foundation",
      "streamConfig": {
        "assetPaths": [],
        "streamWeb": true,
        "streamWebFolder": "web9",
        "webSourcePath": "src"
      }
    }
  ]
}

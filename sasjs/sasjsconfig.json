{
  "$schema": "https://raw.githubusercontent.com/sasjs/utils/main/src/types/sasjsconfig-schema.json",
  "macroFolders": [
    "sasjs/macros"
  ],
  "serviceConfig": {
    "serviceFolders": [
      "sasjs/services/common"
    ]
  },
  "syncFolder": "sasjs/mocks",
  "defaultTarget": "viya",
  "targets": [
    {
      "name": "viya",
      "serverUrl": "",
      "serverType": "SASVIYA",
      "appLoc": "/Public/app/minimal-seed-app",
      "buildConfig": {
        "buildOutputFileName": "myviyadeploy.sas"
      },
      "httpsAgentOptions": {
        "allowInsecureRequests": false
      },
      "streamConfig": {
        "streamWeb": true,
        "streamWebFolder": "web",
        "webSourcePath": "src"
      }
    },
    {
      "name": "sas9",
      "serverType": "SAS9",
      "appLoc": "/Public/app/minimal-seed-app",
      "buildConfig": {
        "buildOutputFileName": "mysas9deploy.sas"
      },
      "deployConfig": {
        "deployServicePack": true
      },
      "serverName": "SASApp",
      "repositoryName": "Foundation",
      "streamConfig": {
        "streamWeb": true,
        "streamWebFolder": "web9",
        "webSourcePath": "src"
      }
    },
    {
      "name": "server",
      "serverType": "SASJS",
      "appLoc": "/Public/app/minimal-seed-app",
      "streamConfig": {
        "streamLogo": "favicon.ico",
        "streamWebFolder": "web",
        "streamWeb": true,
        "webSourcePath": "src",
        "streamServiceName": "minimal-seed-app"
      }
    }
  ]
}
